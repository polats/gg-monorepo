{
  "mcpServers": {
    "devvit-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@devvit/mcp"
      ],
      "disabled": false
    },
    "vercel": {
      "url": "https://mcp.vercel.com"
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "disabled": false,
      "autoApprove": [
        "browser_take_screenshot",
        "browser_wait_for",
        "browser_console_messages",
        "browser_evaluate",
        "browser_navigate",
        "browser_network_requests",
        "browser_close",
        "browser_click",
        "browser_select_option",
        "browser_handle_dialog"
      ]
    }
  }
}